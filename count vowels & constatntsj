public class Main {
    public static void main(String[] args) {
        String s="rohith";
        String result="";
        int vowels=0;
        int constants=0;
        for(int i=0;i<s.length();i++)
        {
            char ch=s.charAt(i);
            if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
            {
                vowels++;
            }
            else{
                constants++;
            }
        }
        System.out.println(vowels);
        System.out.println(constants);
    }
}
