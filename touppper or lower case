import java.util.*;
public class Main{
    public static void main(String[] args)
    {
        String s="ROHITH";
        String e="";
        for(int i=0;i<s.length();i++)
        {
            char ch=s.charAt(i);
            if(ch>='A' && ch<='Z')
            {
                e+=(char)(ch+32);
            }
        }
        System.out.println(e);
        
    }
}
